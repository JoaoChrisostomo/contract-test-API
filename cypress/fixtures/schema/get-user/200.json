{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "default": {},
    "required": [
        "quantidade",
        "usuarios"
    ],
    "properties": {
        "quantidade": {
            "type": "integer",
            "default": 0
        },
        "usuarios": {
            "type": "array",
            "default": [],
            "additionalItems": true,
            "items": {
                "type": "object",
                "default": {},
                "required": [
                    "nome",
                    "email",
                    "password",
                    "administrador",
                    "_id"
                ],
                "properties": {
                    "nome": {
                        "type": "string",
                        "default": ""
                    },
                    "email": {
                        "type": "string",
                        "default": ""
                    },
                    "password": {
                        "type": "string",
                        "default": ""
                    },
                    "administrador": {
                        "type": "string",
                        "default": ""
                    },
                    "_id": {
                        "type": "string",
                        "default": ""
                    }
                },
                "additionalProperties": true
            }
        }
    },
    "additionalProperties": true
}